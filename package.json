{
  "name": "screen",
  "version": "0.0.0",
  "private": true,
  "main": "out/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "electron-dev": "electron-vite serve --watch",
    "electron-preview": "concurrently \"bun dev\" \"electron-vite preview\""
  },
  "dependencies": {
    "@types/electron": "^1.6.12",
    "concurrently": "^9.1.1",
    "electron": "^33.2.1",
    "electron-vite": "^2.3.0",
    "element-plus": "^2.9.2",
    "js-web-screen-shot": "^1.9.9-rc.24",
    "lodash": "^4.17.21",
    "pinia": "^2.3.0",
    "sass": "^1.83.1",
    "ts-node": "^10.9.2",
    "uuid": "^11.0.3",
    "vue": "^3.5.13",
    "vue-cropper": "^0.6.5",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.20",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  }
}
